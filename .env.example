cat > .env.example << 'EOF'
# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=arachne
POSTGRES_USER=arachne
POSTGRES_PASSWORD=your_secure_password_here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Tor Configuration
TOR_SOCKS_PORT=9050
TOR_CONTROL_PORT=9051
TOR_PASSWORD=your_tor_control_password

# Security
ENCRYPTION_KEY=your_32_character_encryption_key_here
AIR_GAP_MODE=true

# Operational
LOG_LEVEL=INFO
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT=30

# Legal Compliance
ENABLE_SAFETY_FILTERS=true
ILLEGAL_CONTENT_FILTER=strict
REQUIRE_HUMAN_REVIEW=true

# Note: Never commit actual secrets to version control
EOF
